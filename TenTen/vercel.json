{
  "version": 2,
  "builds": [
    {
      "src": "wwwroot/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/_framework/(.*)",
      "dest": "/_framework/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/css/$1"
    },
    {
      "src": "/lib/(.*)",
      "dest": "/lib/$1"
    },
    {
      "src": "/sample-data/(.*)",
      "dest": "/sample-data/$1"
    },
    {
      "src": "/favicon.png",
      "dest": "/favicon.png"
    },
    {
      "src": "/icon-192.png",
      "dest": "/icon-192.png"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}