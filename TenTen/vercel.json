{
  "version": 2,
  "buildCommand": "dotnet publish -c Release -o output",
  "outputDirectory": "output/wwwroot",
  "framework": "blazor",
  "builds": [
    {
      "src": "TenTen/TenTenApp.csproj",
      "use": "@vercel/dotnet"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}